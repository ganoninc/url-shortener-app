{
  "name": "vite-url-shortener-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "storybook": "storybook dev -p 6006 --disable-telemetry",
    "build-storybook": "storybook build",
    "gen:url-service": "openapi-generator-cli generate -g typescript-axios -o src/api/url/generated -i http://localhost:8082/v3/api-docs --additional-properties=supportsES6=true,skipFormModel=false"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "axios": "^1.10.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.7.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/js": "^9.30.1",
    "@openapitools/openapi-generator-cli": "^2.23.1",
    "@storybook/addon-a11y": "^9.1.5",
    "@storybook/addon-docs": "^9.1.4",
    "@storybook/addon-onboarding": "^9.1.5",
    "@storybook/addon-vitest": "^9.1.5",
    "@storybook/react-vite": "^9.1.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.15",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/redux-mock-store": "^1.5.0",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^9.1.4",
    "globals": "^16.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-environment-jsdom": "^30.0.5",
    "jest-transformer-svg": "^2.1.0",
    "jsdom": "^26.1.0",
    "msw": "^2.11.2",
    "playwright": "^1.55.0",
    "redux-mock-store": "^1.5.5",
    "storybook": "^9.1.4",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vitest": "^3.2.4"
  }
}
